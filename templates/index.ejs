<div class="row">
    <div class="col s12 center-align">
        <img src="{{APP_SERVICE_PATH}}/images/logo.png" alt="Logo" class="img-responsive">
        <h6 style="color: #4d4d4d">Buscador de anuncios clasificados en Cuba</h6>
    </div>

    <div class="col input-field outline search-field">
        <i class="material-icons" onclick="sendData()">search</i>
        <input id="q" type="text" class="search-input" onkeyup="clickSearch()"/>
        <label for="q">Buscar</label>
    </div>

    <div class="col s12 categories">
        <a class="col s2" onclick="sendDataCategory('casas','1')">
            <i class="material-icons amber darken-1">home</i>
            <br>
            <small>Casas</small>
        </a>
        <a class="col s2" onclick="sendDataCategory('autos','1')">
            <i class="material-icons blue accent-4">directions_car</i>
            <br>
            <small>Autos</small>
        </a>
        <a class="col s2" onclick="sendDataCategory('electronica','1')">
            <i class="material-icons red darken-3">tv</i>
            <br>
            <small>Electronica</small>
        </a>
        <a class="col s2" onclick="sendDataCategory('servicios','1')">
            <i class="material-icons amber darken-1">view_list</i>
            <br>
            <small>Servicios</small>
        </a>
        <a class="col s2" onclick="sendDataCategory('ventas','1')">
            <i class="material-icons green darken-2">trending_up</i>
            <br>
            <small>Ventas</small>
        </a>
        <a class="col s2" onclick="sendDataCategory('empleos','1')">
            <i class="material-icons blue accent-4">business_center</i>
            <br>
            <small>Empleos</small>
        </a>
    </div>
</div>